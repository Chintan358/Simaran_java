package pettern;

public class P9 {
	public static void main(String[] args) {
		
		
		int lines = 9 ;
		int mid = (lines/2)+1
		for(int i=1;i<=lines;i++)
		{
			for(int j=1;j<=lines;j++)
			{
				if(i==1 || j==mid)
				{
				System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
		
		
	}
}
